{
  "name": "pherrit",
  "version": "0.0.1",
  "description": "A browser extension that adds Gerrit patches to the Wikimedia Phabricator UI",
  "main": "src/main.js",
  "scripts": {
    "start": "concurrently --prefix=none --raw 'node proxy.js' 'open http://localhost:3000/' 'webpack --watch --mode=development'",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j4n-co/pherrit.git"
  },
  "keywords": [
    "Wikimedia",
    "Phabricator",
    "Gerrit"
  ],
  "author": "Jan Drewniak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/j4n-co/pherrit/issues"
  },
  "homepage": "https://github.com/j4n-co/pherrit#readme",
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.1.0",
    "cors": "2.8.4",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "^1.0.0",
    "eslint": "^5.7.0",
    "eslint-config-wikimedia": "^0.7.2",
    "eslint-plugin-qunit": "^3.3.1",
    "express": "^4.16.4",
    "jsdoc": "^3.5.5",
    "mini-css-extract-plugin": "^0.4.4",
    "node-fetch": "^2.3.0",
    "postcss": "^7.0.5",
    "postcss-color-function": "^4.0.1",
    "postcss-custom-properties": "^7.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.23.0",
    "webpack-cli": "^3.1.2",
    "webpack-shell-plugin": "^0.5.0",
    "webpack-zip-plugin": "^0.1.6",
    "wikimedia-ui-base": "^0.12.0",
    "zip-webpack-plugin": "^3.0.0"
  }
}
